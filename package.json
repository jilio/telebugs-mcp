    "build:linux": "bun build --compile --target=bun-linux-x64 --outfile=telebugs-mcp-linux src/index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "express": "^5.2.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "bun-types": "^1.3.6",
    "typescript": "^5.9.3"
  }
}
